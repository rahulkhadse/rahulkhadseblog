</head>
<body id="single">
    <div class="content-body" id="mainBody" style="display:none;">
        <div class="container">
            <div class="row">
                <aside class="col-lg-3 col-md-3 col-sm-12 col-xs-12" id="sideNavbar">
                    <div class="widget widget-recent-posts">
                        <h3 class="widget-title">
                            <span class="badge blogIcon">blog</span> &nbsp;<i class="blogger">by</i> &nbsp;
                        </h3>

                        <h1 class="widget-title blogTitle">
                            Rahul <br />Khadse
                        </h1>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <input type="text" class="form-control" id="search" maxlength="50" placeholder="search" onchange="search()">@*<button id="searchBtn" class="btn btn-default"><i class="fa fa-search"></i></button>*@
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <nav>
                                    <a href="/" class="navbtn @if(ViewBag.ActiveButton == "Home") {<text>navbtn-active</text>} showloading"><i class="fa fa-home"></i><br />Home</a>
                                    <a href="@Url.Action("List", "Archive")" class="navbtn @if(ViewBag.ActiveButton == "Archive") {<text>navbtn-active</text>} showloading"><i class="fa fa-archive"></i><br />Archive</a><br />
                                    <a href="https://github.com/rahulkhadse" target="_blank" class="navbtn @if(ViewBag.ActiveButton == "Code") {<text>navbtn-active</text>} showloading"><i class="fa fa-code"></i><br />Code</a>
                                    <a href="@Url.Action("Me", "Home")" class="navbtn @if(ViewBag.ActiveButton == "Me") {<text>navbtn-active</text>} showloading"><i class="fa fa-info-circle"></i><br />&nbsp;&nbsp;Me&nbsp;&nbsp;</a>
                                </nav>
                                <br />
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <a target="_blank" class="socialMediaLinks" href="mailto:rkhadse@kent.edu"><i class="fa fa-envelope-o fa-2x" aria-hidden="true"></i></a>&nbsp;&nbsp;
                                <a target="_blank" class="socialMediaLinks" href="https://twitter.com/rahulkhadse"><i class="fa fa-twitter-square fa-2x" aria-hidden="true"></i></a>&nbsp;&nbsp;
                                <a target="_blank" class="socialMediaLinks" href="https://github.com/rahulkhadse"><i class="fa fa-github-square fa-2x" aria-hidden="true"></i></a>&nbsp;&nbsp;
                                <a target="_blank" class="socialMediaLinks" href="https://jsfiddle.net/user/rahul_khadse/fiddles/"><i class="fa fa-jsfiddle fa-2x" aria-hidden="true"></i></a>
                            </div>
                        </div>
                        @*<div class="row">
                                <div class="col-lg-12">
                                    <h3 class="widget-title">Archives</h3>
                                    <ul>
                                        <li>
                                            <a href="#">November 2014</a>
                                        </li>
                                        <li>
                                            <a href="#">September 2014</a>
                                        </li>
                                        <li>
                                            <a href="#">January 2013</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-12">
                                    <h3 class="widget-title">Category</h3>
                                    <ul>
                                        <li>
                                            <a href="#">Web Design</a>
                                        </li>
                                        <li>
                                            <a href="#">Web Development</a>
                                        </li>
                                        <li>
                                            <a href="#">SEO</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>*@
                    </div>
                </aside>
                <div class="col-lg-1 col-md-1"></div>
                <main class="col-lg-7 col-md-7 col-sm-12 col-xs-12" id="mainNavbar">
                    @RenderBody()
                    <br />
                    <footer>
                        &copy; 2016
                        <script>new Date().getFullYear() > 2016 && document.write("-" + new Date().getFullYear());</script>

                        <br /> <a rel="license" href="http://creativecommons.org/licenses/by-nc-nd/4.0/"><img alt="Creative Commons License" style="border-width:0" src="https://i.creativecommons.org/l/by-nc-nd/4.0/88x31.png" /></a><br />This work is licensed under a <a rel="license" href="http://creativecommons.org/licenses/by-nc-nd/4.0/">Creative Commons Attribution-NonCommercial-NoDerivatives 4.0 International License</a>.
                    </footer>
                </main>
                <div class="col-lg-1 col-md-1"></div>
            </div>
        </div>
    </div>
    <div id="loadingmodalDiv" style="height: 2500px;background-color: #444;text-align: center;">
        <img class="modal-content img-responsive" alt="Loading" src="~/images/loading2.gif" style="margin-top: 8em;max-width: 90%;" />
    </div>
    @RenderSection("scripts", required: false)
    @RenderSection("templates", required: false)
    <script>
        var loadingFn = function () {
            var getEle = document.getElementById("mainBody");
            getEle.style.display = "none";
            getEle = document.getElementById("loadingmodalDiv");
            getEle.style.display = "";
        };
        function search() {
            loadingFn();
            window.location = "/Archive/Search?id=" + document.getElementById("search").value;
        }
        (function () {
            var getEle = document.getElementById("mainBody");
            getEle.style.display = "";
            getEle = document.getElementById("loadingmodalDiv");
            getEle.style.display = "none";
            setTimeout(function () {
                var showloadingSubcribers = document.getElementsByClassName("showloading");
                for (var i = 0; i < showloadingSubcribers.length; i++) {
                    //console.log(showloadingSubcribers[i]);
                    showloadingSubcribers[i].addEventListener('click', loadingFn, false);
                }
            }, 3000);

        })();
    </script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-79459711-2', 'auto');
        ga('send', 'pageview');
    </script>
    <script id="dsq-count-scr" src="//blograhulkhadsecom.disqus.com/count.js" async></script>
</body>

